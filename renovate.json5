{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
  ],
  ignorePaths: [
    "**/*.sops.*",
  ],
  flux: {
    fileMatch: [
      "(^|/)kubernetes/.+\\.ya?ml$"
    ]
  },
  "helm-values": {
    fileMatch: [
      "(^|/)kubernetes/.+\\.ya?ml$"
    ]
  },
  kubernetes: {
    fileMatch: [
      "(^|/)kubernetes/.+\\.ya?ml$"
    ]
  }
}